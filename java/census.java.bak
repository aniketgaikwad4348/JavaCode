class census
{
 public static void main(String[] args)
 {
  int currPop = 312032486;
  int second = (365 * 24 * 60 * 60) *5 ; 
  int birth = second / 7 ;
  int death = second / 13 ; 
  int imm = second / 45 ;
  
  int newPop = currPop + birth - death + imm ;
  System.out.println("CURRENT POPULATION : " +	currPop);
  System.out.println("POPULATION AFTER 5 YEAR :" + newPop);
 }
}